<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
    <NoWarn>$(NoWarn);SA1000;SA0001;SA1009;SA1010;SA1402;SA1413;SA1515;SA1516;SA1600;SA1649;CS8618</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="../src/EFCore.SqlServer/EFCore.SqlServer.csproj" />
    <ProjectReference Include="../src/EFCore.Cosmos/EFCore.Cosmos.csproj" />
  </ItemGroup>

</Project>
